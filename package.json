{
	"name": "arucas",
	"displayName": "Arucas Language Extension",
	"description": "Syntax highlighting support for Arucas",
	"version": "1.0.5",
	"publisher": "Arucas",
  "icon": "assets/icon.png",
	"engines": {
		"vscode": "^1.71.0"
	},
	"categories": [
		"Programming Languages"
	],
	"author": {
		"name": "senseiwells"
	},
	"repository": {
		"url": "https://github.com/senseiwells/Arucas-VSCode-Extension"
	},
	"contributes": {
		"languages": [
			{
				"id": "arucas",
				"aliases": [
					"Arucas",
					"arucas"
				],
				"extensions": [
					".arucas"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "arucas",
				"scopeName": "source.arucas",
				"path": "./syntaxes/arucas.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "arucas",
				"path": "./snippets/arucas.json"
			}
		]
	},
